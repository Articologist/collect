<div id="additionnalFields">
    <div id="data" class="data-holder block-element hidden">
        <h6 class="header">{{ 'title.data'|trans }}</h6>
        {% for key, datum in data if datum.type in ([constant('App\\Enum\\DatumTypeEnum::TYPE_TEXT'), constant('App\\Enum\\DatumTypeEnum::TYPE_COUNTRY')]) %}
            {{ include('App/Datum/datum.html.twig', {datum: datum, iteration: key, type: datum.type}) }}
        {% endfor %}
    </div>

    <div id="item-images" class="data-holder block-element hidden">
        <h6 class="header">{{ 'title.item_images'|trans }}</h6>
        {% for key, datum in data if datum.type in ([constant('App\\Enum\\DatumTypeEnum::TYPE_IMAGE'), constant('App\\Enum\\DatumTypeEnum::TYPE_SIGN')]) %}
            {{ include('App/Datum/datum.html.twig', {datum: datum, iteration: key, type: datum.type}) }}
        {% endfor %}
    </div>
</div>