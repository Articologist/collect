{
    "minimum-stability": "dev",
    "name": "koillection/koillection",
    "description": "Koillection is a self-hosted service allowing users to manage any kind of collection.",
    "type": "project",
    "license": "GPL-3.0-only",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "maennchen/zipstream-php": "^1.2",
        "php-http/curl-client": "^1.7",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "v5.0.0-BETA2",
        "symfony/console": "v5.0.0-BETA2",
        "symfony/dotenv": "v5.0.0-BETA2",
        "symfony/expression-language": "v5.0.0-BETA2",
        "symfony/flex": "^1.1",
        "symfony/form": "v5.0.0-BETA2",
        "symfony/framework-bundle": "v5.0.0-BETA2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "v5.0.0-BETA2",
        "symfony/security-bundle": "v5.0.0-BETA2",
        "symfony/serializer-pack": "*",
        "symfony/translation": "v5.0.0-BETA2",
        "symfony/twig-bundle": "v5.0.0-BETA2",
        "symfony/validator": "v5.0.0-BETA2",
        "symfony/web-link": "v5.0.0-BETA2",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "v5.0.0-BETA2",
        "twig/extensions": "^1.5",
        "willdurand/js-translation-bundle": "^2.6",
        "willdurand/negotiation": "^2.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "php-translation/symfony-bundle": "^0.9.1",
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "v5.0.0-BETA2"
        }
    }
}
